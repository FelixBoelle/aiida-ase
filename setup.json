    "license": "MIT License",
    "author": "Sebastiaan P. Huber",
    "author_email": "mail@sphuber.net",
    "description": "The AiiDA plugin for ASE",
    "classifiers": [
        "License :: OSI Approved :: MIT License",
        "Programming Language :: Python :: 2.7",
